<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id = "alien1" class = "com.abhishek.demo.Alien1" autowire ="byType">
   <constructor-arg value = "butterfly"></constructor-arg>
   <property name = "age">
   <value>
   10
   </value></property>
   
   </bean>
   
   
   <bean id = "laptop" class = "com.abhishek.demo.Laptop">
   <property name = "ram" value = "Samsung"></property>
   <property name = "disk" value ="Samsung"></property>
   </bean>
   
   <bean id ="com" class ="com.abhishek.demo.Desktop" primary = "true">
   </bean>
   
   </beans>